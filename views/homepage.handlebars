<div id="wrap">
  <div class="container">
    <table cellpadding="0" cellspacing="0" border="0" class="datatable table table-striped table-bordered">
      <thead>
        <tr>
          <th>NAME</th>
          <th>PRICE (USD)</th>
          <th>DAY VOLUME (24hr)</th>
          <th>MARKET CAP</th>
        </tr>
      </thead>
      {{#each coinData as |data|}}
      <tr class="gradeA">
        <td>
          {{!-- {{#if data.id === "false"}} --}}
          <button class="favouritesButton" value="{{data.id}}">â™¡</button>
          {{!-- {{/if}} --}}
          <a href="/crptoProfile/{{data.id}}">{{data.name}}</a>
        </td>
        <td>{{data.quote.USD.price}} </td>
        <td>{{data.quote.USD.volume_24h}}</td>
        <td>{{data.quote.USD.market_cap}}</td>
      </tr>
      {{/each}}
      <tbody>

      </tbody>
    </table>
  </div>
</div>

